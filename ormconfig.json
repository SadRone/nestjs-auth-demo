{
  "type": "postgres",
  "host": "localhost",
  "port": 5432,
  "username": "your_db_username",
  "password": "your_db_password",
  "database": "your_db_name",
  "entities": ["src/**/*.entity{.ts,.js}"],
  "migrations": ["src/migrations/*.ts"],
  "cli": {
    "migrationsDir": "src/migrations"
  },
  "synchronize": false
}
